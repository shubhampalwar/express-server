    "Clean:Dist": "rm -rf dist/*",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json --exclude 'node_modules/**/*.{j,t}s'",
    "lint:fix": "yarn lint -- --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
