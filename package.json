  "dependencies": {
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.7",
    "typescript": "^3.2.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.0",
